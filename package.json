{
  "name": "string-incr",
  "version": "3.1.1",
  "description": "Increment or decrement strings with numbers",
  "license": "MIT",
  "author": "",
  "type": "module",
  "keywords": [
    "increment",
    "decrement",
    "string",
    "number",
    "incr",
    "decr",
    "slug"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apoutchika/string-incr.git"
  },
  "bugs": {
    "url": "https://github.com/apoutchika/string-incr/issues"
  },
  "homepage": "https://github.com/apoutchika/string-incr#readme",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "pnpm run lint && pnpm run typecheck && pnpm run test && pnpm run build"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "@vitest/coverage-v8": "^1.2.1",
    "eslint": "^8.56.0",
    "eslint-config-standard-with-typescript": "^43.0.1",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-n": "^16.6.2",
    "eslint-plugin-promise": "^6.1.1",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3",
    "vitest": "^1.2.1"
  },
  "packageManager": "pnpm@8.15.1"
}
